{
  "name": "@culturecoded/sdk",
  "version": "1.0.1",
  "description": "Official JavaScript/TypeScript SDK for CultureCoded - Cultural UX Intelligence Platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build",
    "version:patch": "node scripts/version.js patch",
    "version:minor": "node scripts/version.js minor",
    "version:major": "node scripts/version.js major",
    "test": "echo \"No tests yet\""
  },
  "keywords": [
    "culturecoded",
    "cultural-ux",
    "design",
    "localization",
    "internationalization",
    "figma",
    "sdk",
    "api"
  ],
  "author": "CultureCoded",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/culturecoded/sdk-js"
  },
  "homepage": "https://culturecoded.io",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
